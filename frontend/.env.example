# Frontend Environment Variables
# Copy this file to .env for local development

# API URL - Backend endpoint
# For Docker: use http://localhost:5010
# For local dev: use http://localhost:5000
REACT_APP_API_URL=http://localhost:5010

# WebSocket Port (for hot reload in development with Docker)
# Should match the port exposed on the host
WDS_SOCKET_PORT=3010
